{
  "name": "reggex",
  "version": "0.5.2",
  "description": "A TypeScript library that provides a programmatic and human readable way to create strongly-typed regular expressions",
  "author": "P. Christopher Bowers",
  "license": "MIT",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/pcbowers/reggex.git"
  },
  "bugs": {
    "url": "https://github.com/pcbowers/reggex/issues"
  },
  "keywords": [
    "regular",
    "expressions",
    "javascript",
    "typescript",
    "typed"
  ],
  "tsup": {
    "entry": [
      "src/index.ts",
      "src/transform.ts"
    ],
    "treeshake": true,
    "minify": true,
    "clean": true,
    "dts": true,
    "format": [
      "cjs",
      "esm"
    ]
  },
  "module": "./dist/index.mjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./transform": {
      "require": "./dist/transform.js",
      "import": "./dist/transform.mjs",
      "types": "./dist/transform.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vitest",
    "build": "tsup",
    "test": "vitest run",
    "check-exports": "check-export-map",
    "lint": "prettier --check . && TIMING=1 eslint --ignore-path .gitignore .",
    "publish-packages": "changeset version && changeset publish",
    "ci": "turbo run lint test build check-exports",
    "release": "turbo run publish-packages"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/node": "^20.8.9",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitest/coverage-v8": "^0.34.6",
    "check-export-map": "^1.3.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "3.2.5",
    "tsup": "^8.0.0",
    "turbo": "^1.10.16",
    "typescript": "^5.2.2",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "@types/estree": "^1.0.3",
    "acorn": "^8.11.2",
    "estree-walker": "^3.0.3",
    "magic-string": "^0.30.5",
    "mlly": "^1.4.2",
    "unplugin": "^1.5.0"
  }
}
